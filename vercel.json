{
  "version": 2,
  "builds": [
    {
      "src": "Project/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "Project/backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Project/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "Project/frontend/build/$1"
    }
  ],
  "outputDirectory": "Project/frontend/build"
}
